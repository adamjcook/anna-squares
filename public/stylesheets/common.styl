// common.styl
// Root Stylus stylesheet source for anna-squares

body
  font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
  padding-top: 45px;
  background: #515151;

b,
strong
  font-weight: bold;

h1,
h2,
h3,
h4,
h5,
h6,
.h1,
.h2,
.h3,
.h4,
.h5,
.h6
  font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
  font-weight: normal;

a,
a:hover,
a:active,
a:focus,
button
  outline: 0;
  outline: none;

.jumbotron
  font-weight: normal;

.jumbotron > h3
  margin-bottom: 21px;

.navbar
  font-weight: normal;

.navbar .dropdown-menu > li > a,
.navbar .dropdown-menu > li > a:focus
  font-weight: normal;

.dropdown-menu > li > a:hover,
.dropdown-menu > li > a:focus
  color: #333;
  background-color: #cfcfcf;
  cursor: pointer;

.navbar-inverse .navbar-brand
  color: #fff;

.navbar-inverse .navbar-nav > li > a
  color: #fff;

.navbar-inverse .navbar-nav > li > a:hover,
.navbar-inverse .navbar-nav > li > a:focus
  color: #333;
  background-color: #fff;

.navbar-inverse .navbar-nav > li > a:active
  color: #333;
  background-color: #fff;

label,
.control-label,
.help-block,
.checkbox,
.radio
  font-size: 13px;
  font-weight: bold;

hr
  margin-top: 0;
  margin-bottom: 5px;
  border-top: 1px solid #ccc;

pre
  border-radius: 0;


.alert
  border-radius: 0;

.as-container-full
  width: 100%;
  min-height: 45px;
  margin: 0;
  
.as-jumbotron-top
  background: #B00000;

.as-jumbotron-bottom
  background: #3f4245;

.as-container-full .jumbotron
  padding: 0;
  background: transparent;
  color: #fff;

.as-btn-hollow
  background: none;
  color: #fff;

.btn-md.as-btn-hollow
  padding: 10px 28px;

.btn
  border-radius: 0;
  color: #333;

.btn-warning:hover,
.btn-warning:focus,
.btn-warning:active,
.btn-warning.active,
.open .dropdown-toggle.btn-warning
  color: #333;

.btn-danger:hover,
.btn-danger:focus,
.btn-danger:active,
.btn-danger.active,
.open .dropdown-toggle.btn-danger
  color: #333;

.navbar-nav>li.as-accent
  background: #484848;

.form-control
  border-radius: 0;

.glyphicon
  color: #333;

.well
  margin-bottom: 10px;
  background: #cfcfcf;
  border: 0;
  border-radius: 0;
  -webkit-box-shadow: none;
  box-shadow: none;

.well-lg
  border-radius: 0;

.well-sm
  border-radius: 0;

.as-content-container
  background: #fff;

.as-noborder
  border: none;

.as-footer
  padding-top: 10.5px;
  font-size: 13px;

.as-footer a,
.as-footer a:hover,
.as-footer a:focus
  color: #000;
  text-decoration: none;

.as-footer .as-copyright
  color: #999;

.as-footer .as-license
  color: #999;

.as-form
  font-size: 13px;

.as-form p
  line-height: 1.1;

.as-form .as-field-help
  margin-top: 5px;

.panel-default > .panel-heading
  background-color: #dadada;
  border-color: #dadada;


.as-sidebar.affix
  position: static;

.as-sidenav
  margin-top: 30px;
  margin-bottom: 30px;
  padding-top:    10px;
  padding-bottom: 10px;
  background-color: #fff;
  border-radius: 0px;

.as-sidebar .nav > li > a
  display: block;
  color: #333;
  padding: 5px 20px;
  border-left: 3px solid transparent;

.as-sidebar .nav > li > a:hover,
.as-sidebar .nav > li > a:focus
  text-decoration: none;
  color: #333;
  background-color: #cfcfcf;
  border-left: 3px solid #9e9e9e;
  font-weight: normal;

.as-sidebar .nav > .active > a,
.as-sidebar .nav > .active:hover > a,
.as-sidebar .nav > .active:focus > a
  font-weight: bold;
  color: #fff;
  background-color: #9e9e9e;
  border-left: 3px solid #333;
  font-weight: normal;

.as-sidebar .nav .nav
  display: none;
  margin-bottom: 8px;

.as-sidebar .nav .nav > li > a
  padding-top: 3px;
  padding-bottom: 3px;
  padding-left: 30px;
  font-size: 90%;

@media (min-width: 992px)
  .as-sidebar .nav > .active > ul
    display: block;

  .as-sidebar.affix,
  .as-sidebar.affix-bottom
    width: 213px;

  .as-sidebar.affix-bottom
    position: aasolute;

  .as-sidebar.affix-bottom .as-sidenav,
  .as-sidebar.affix .as-sidenav
    margin-top: 0;
    margin-bottom: 0;

@media (min-width: 1200px)
  .as-sidebar.affix-bottom,
  .as-sidebar.affix
    width: 263px;


.as-btn-offset
  margin-top: 22px;

.as-schedule-item-well
  margin-top: 15px;

.as-schedule-items-list
  margin-bottom: 0;
  padding-left: 0;

.as-schedule-item-row
  margin: 10px 0;

.btn-group > button
  margin-right 5px;